-- Tap Simulator (Cursor Makers) ✅ WORKING GUI - February 2026
-- FIXED: Uses EXACT remote path from real scripts - ReplicatedStorage["73bd9f38-0e90-4df4-bdd9-dfc8d55ec1e1"].Events[19]
-- Auto finds the 20th RemoteEvent (index 19) - TAP/CLICK remote (hatch needs gamepass, this = auto money for eggs)
-- Black BG + White Outline, FULLY DRAGGABLE
-- Buttons: Auto Hatch (fast spam = no anim), No Anim Hatch (ultra fast), 60x Hatch (burst)

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = Players.LocalPlayer

local sg = Instance.new("ScreenGui")
sg.Name = "TapSimExploit"
sg.Parent = game:GetService("CoreGui")
sg.ResetOnSpawn = false

-- Main Frame
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 260, 0, 220)
mainFrame.Position = UDim2.new(0.5, -130, 0.5, -110)
mainFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
mainFrame.BorderSizePixel = 3
mainFrame.BorderColor3 = Color3.fromRGB(255, 255, 255)
mainFrame.Active = true
mainFrame.Draggable = true  -- ✅ PERFECTLY DRAGGABLE
mainFrame.Parent = sg

-- Title
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, -40, 0, 35)
title.Position = UDim2.new(0, 5, 0, 5)
title.BackgroundTransparency = 1
title.Text = "Tap Simulator (Cursor Makers)"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamBold
title.TextSize = 16
title.TextXAlignment = Enum.TextXAlignment.Left
title.Parent = mainFrame

-- Close
local closeBtn = Instance.new("TextButton")
closeBtn.Size = UDim2.new(0, 30, 0, 30)
closeBtn.Position = UDim2.new(1, -35, 0, 5)
closeBtn.BackgroundColor3 = Color3.fromRGB(50, 0, 0)
closeBtn.BorderSizePixel = 1
closeBtn.BorderColor3 = Color3.fromRGB(255, 255, 255)
closeBtn.Text = "X"
closeBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextSize = 16
closeBtn.Parent = mainFrame
closeBtn.MouseButton1Click:Connect(function() sg:Destroy() end)

-- Status
local statusLabel = Instance.new("TextLabel")
statusLabel.Size = UDim2.new(1, -10, 0, 20)
statusLabel.Position = UDim2.new(0, 5, 0, 45)
statusLabel.BackgroundTransparency = 1
statusLabel.Text = "Loading remotes..."
statusLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
statusLabel.Font = Enum.Font.Gotham
statusLabel.TextSize = 13
statusLabel.TextXAlignment = Enum.TextXAlignment.Left
statusLabel.Parent = mainFrame

-- Toggle Creator
local function createToggle(name, posY, callback)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0.92, 0, 0, 45)
    btn.Position = UDim2.new(0.04, 0, 0, posY)
    btn.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    btn.BorderSizePixel = 2
    btn.BorderColor3 = Color3.fromRGB(255, 255, 255)
    btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 15
    btn.Text = name .. ": OFF"
    btn.Parent = mainFrame
    
    local toggled = false
    btn.MouseButton1Click:Connect(function()
        toggled = not toggled
        btn.Text = name .. ": " .. (toggled and "ON" or "OFF")
        btn.BackgroundColor3 = toggled and Color3.fromRGB(0, 150, 0) or Color3.fromRGB(20, 20, 20)
        callback(toggled)
    end)
end

-- ✅ FIXED REMOTE FINDER - EXACT PATH FROM 2026 SCRIPTS
local obfFolder = ReplicatedStorage:WaitForChild("73bd9f38-0e90-4df4-bdd9-dfc8d55ec1e1", 10)
local eventsFolder = obfFolder and obfFolder:WaitForChild("Events", 5)
local tapRemote = nil

if eventsFolder then
    local events = eventsFolder:GetChildren()
    if #events >= 19 then  -- Index 19 = 20th event (TAP remote)
        tapRemote = events[19]
        statusLabel.Text = "✅ TAP Remote Found: " .. tapRemote.Name .. " (Index 19)"
        statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
    else
        statusLabel.Text = "❌ Not enough events (need 20+)"
        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
else
    statusLabel.Text = "❌ Obfuscated folder not found"
    statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
end

-- Toggles
local autoRunning = false
local noAnimRunning = false
local fast60xRunning = false

-- AUTO HATCH (Normal speed tap spam → money for eggs)
spawn(function()
    while task.wait(0.3) do
        if autoRunning and tapRemote then
            pcall(function()
                tapRemote:FireServer(true, nil, false)  -- ✅ EXACT ARGS FROM SCRIPTS
            end)
        end
    end
end)

-- NO ANIM HATCH (Fast spam → feels like no anim)
spawn(function()
    while task.wait(0.05) do
        if noAnimRunning and tapRemote then
            pcall(function()
                tapRemote:FireServer(true, nil, false)
            end)
        end
    end
end)

-- 60x HATCH (Ultra burst → HIGH BAN RISK)
spawn(function()
    while task.wait(0.2) do
        if fast60xRunning and tapRemote then
            for i = 1, 60 do
                pcall(function()
                    tapRemote:FireServer(true, nil, false)
                end)
                task.wait(0.001)
            end
        end
    end
end)

-- Buttons
createToggle("Auto Hatch", 75, function(state) autoRunning = state end)
createToggle("No Anim Hatch", 125, function(state) noAnimRunning = state end)
createToggle("60x Hatch", 175, function(state) fast60xRunning = state end)

print("✅ Tap Simulator GUI LOADED - Uses REAL remote [19]!")
print("Status:", statusLabel.Text)
print("Toggle ON → Watch taps/money skyrocket → Buy & hatch eggs manually!")

-- Auto rescan if needed
spawn(function()
    while task.wait(15) do
        if not tapRemote then
            -- Retry find
            local newEvents = eventsFolder and eventsFolder:GetChildren()
            if newEvents and #newEvents >= 19 then
                tapRemote = newEvents[19]
                statusLabel.Text = "✅ Re-found TAP Remote!"
                statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
            end
        end
    end
end)
